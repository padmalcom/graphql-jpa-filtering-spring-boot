type Query {

    allCars(
        page: Int,
        size: Int,
        filter: [FilterParameter],
        sort: [SortParameter]
    ): [Car]!
}

type Car {
    id: ID
    name: String
    value: Int
    builtAt: LocalDate
    type: CarType
    oldtimer: Boolean
    owners: [Owner]
}

type Owner {
    id: ID
    firstName: String
    lastName: String
}

enum CarType {
    SPORTS
    CABRIOLET
}